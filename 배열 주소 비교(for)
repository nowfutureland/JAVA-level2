public class Main2 {

    public static void main(String[] args) {
        String 변수 = "정,연,택";
        String[] arr = 변수.split(",");

        for(int i=0; i<arr.length;i++) { //python의 for i in 느낌
            System.out.println(arr[i]);
        }

        for(String i : arr) {
            System.out.println(i);
        }
    }
}
